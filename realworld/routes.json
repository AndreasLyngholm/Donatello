    "": {
        "service": "home.ol"
    },
    "test": {
        "service": "test.ol"
    },
    "articles/create": {
        "service": "create_article.ol"
    },
    "settings": {
        "service": "settings.ol"
    },
    "articles/{id}": {
        "service": "article.ol",
        "data": {
