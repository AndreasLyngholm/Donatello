        },
    },
    "profiles/{id}": {
        "service": "profile.ol",
        "data": {
            "identifier": "id",
            "source": "data/users.json",
            "variable": "user"
        },
    },
    "login": {
        "service": "login.ol"
    },
    "session": {
        "service": "session.ol"
    },
    "register": {
        "service": "register.ol"
    },
    "logout": {
        "service": "logout.ol"
    },
}