{
    "": {
        "service": "home.ol"
    },
    "test": {
        "service": "test.ol",
        "data": [
            {
                "identifier": "token",
                "source": "cookie",
                "variable": "token"
            },
            {
                "identifier": "token",
                "source": "cookie",
                "variable": "token_test"
            }
        ]
    },
    "articles/create": {
        "service": "create_article.ol"
    },
    "settings": {
        "service": "settings.ol"
    },
    "articles/{id}": {
        "service": "article.ol",
        "data": [
            {
                "variable": "slug"
            }
        ]
    },
    "profiles/{id}": {
        "service": "profile.ol",
        "data": [
            {
                "identifier": "id",
                "source": "data/users.json",
                "variable": "user"
            }
        ]
    },
    "login": {
        "service": "login.ol"
    },
    "session": {
        "service": "session.ol"
    },
    "register": {
        "service": "register.ol"
    },
    "logout": {
        "service": "logout.ol"
    },
}